<div id="contact">
    <div class="heading">
        <h1>HUBUNGI SAYA DI 0812-8323-127</h1>
        <br />
        <h4>atau</h4>
        <br />
        <h1>TINGGALKAN PESAN</h1>
    </div>
    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-sm-6 col-md-6" ng-controller="kirimEmailCtrl">
                <form name="formEmail">
                    <input type="text" id="nama" name="nama" ng-model="nama" size="40" class="contact-text mb-10" required placeholder="Nama Lengkap">
                    <span class="error" ng-show="formEmail.nama.$dirty && !formEmail.nama.$valid">Nama mesti disediakan</span>
                    <br>
                    <input type="text" id="telepon" name="telepon" ng-model="telepon" size="40" class="contact-text mt-20 mb-10" required placeholder="Nomor Telepon Anda">
                    <span class="error" ng-show="formEmail.telepon.$dirty && !formEmail.telepon.$valid">Telepon mesti disediakan</span>
                    <br>
                    <input type="email" id="email" name="email" size="40" ng-model="email" class="contact-text mt-20 mb-10" required placeholder="Alamat Email">
                    <span class="error" ng-show="formEmail.email.$dirty && !formEmail.email.$valid">Email mesti disediakan</span>
                    <br>
                    <input type="text" id="perihal" name="perihal" size="40" ng-model="perihal" class="contact-text mt-20 mb-10" required placeholder="Perihal">
                    <span class="error" ng-show="formEmail.perihal.$dirty && !formEmail.perihal.$valid">Perihal mesti disediakan</span>
                    <br>
                    <textarea name="pesan" cols="40" rows="10" ng-model="pesan" class="contact-message mt-20 mb-10" required placeholder="Pesan Anda"></textarea>
                    <span class="error" ng-show="formEmail.pesan.$dirty && !formEmail.pesan.$valid">Pesan mesti disediakan</span>
                    <br>
                    <input type="submit" ng-click="kirimEmail()" value="KIRIM EMAIL" class="send-btn mt-20" ng-disabled="(formEmail.nama.$pristine||(formEmail.nama.$dirty && !formEmail.nama.$valid))||(formEmail.telepon.$pristine||(formEmail.telepon.$dirty && !formEmail.telepon.$valid))||(formEmail.email.$pristine||(formEmail.email.$dirty && !formEmail.email.$valid))||(formEmail.perihal.$pristine||(formEmail.perihal.$dirty && !formEmail.perihal.$valid))||(formEmail.pesan.$pristine||(formEmail.pesan.$dirty && !formEmail.pesan.$valid))">
                </form>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-6" ng-controller="kirimSMSCtrl">
                <form name="formSMS">
                    <input type="text" id="nama" name="nama" size="40" class="contact-text mb-10" placeholder="Nama Lengkap">
                    <br>
                    <input type="text" id="telepon" name="telepon" size="40" class="contact-text mt-20 mb-10" placeholder="Nomor Telepon Anda">
                    <br>
                    <textarea name="pesan" cols="40" rows="10" class="contact-message mt-20 mb-10" placeholder="Pesan Anda"></textarea>
                    <br>
                    <input type="submit" ng-click="kirimSMS()" value="KIRIM SMS" class="send-btn mt-20">
                </form>
            </div>
        </div>
    </div>
</div>
